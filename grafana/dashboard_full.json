{
  "dashboard": {
    "title": "AutoSciLab Full Dashboard",
    "panels": [
      {
        "type": "graph",
        "title": "Request Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(autoscillab_request_latency_seconds_bucket[5m])) by (le))"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Request Count",
        "targets": [
          {
            "expr": "sum(rate(autoscillab_request_count[5m])) by (method)"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Celery Tasks Pending",
        "targets": [
          {
            "expr": "sum(celery_tasks_pending)"
          }
        ]
      }
    ]
  }
}