{
  "dashboard": {
    "title": "AutoSciLab Enterprise Dashboard",
    "panels": [
      {
        "type": "graph",
        "title": "Request Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(autoscillab_request_latency_seconds_bucket[5m])) by (le))"
          }
        ]
      },
      {
        "type": "stat",
        "title": "Celery Tasks Success Rate",
        "targets": [
          {
            "expr": "sum(rate(celery_task_success_total[5m]))"
          }
        ]
      },
      {
        "type": "table",
        "title": "Latest Tasks",
        "targets": [
          {
            "expr": "autoscillab_task_events_total"
          }
        ]
      }
    ]
  }
}